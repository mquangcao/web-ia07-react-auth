{
  "name": "@app/common",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:clean": "rimraf dist && pnpm build",
    "build": "npx tsc -p tsconfig.json",
    "lint:fix": "eslint src --report-unused-disable-directives --max-warnings 0 --fix",
    "format:fix": "prettier --write \"src/**/*.ts\""
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "dependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@nestjs/swagger": "^11.2.3",
    "config": "^4.1.1"
  },
  "devDependencies": {
    "@app/eslint-config": "^1.0.0",
    "@app/prettier-config": "^1.0.0",
    "@app/typescript-config": "^1.0.0",
    "@types/config": "^3.3.5",
    "typescript": "^5.9.3"
  }
}
